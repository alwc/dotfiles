{
  //"coc.preferences.formatOnSaveFiletypes": ["python"],

  //"diagnostic.displayByAle": true,
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",

  "git.changedSign.text": "",
  "git.addedSign.text": "",
  "git.removedSign.text": "",
  "git.topRemovedSign.text": "ﬢ",
  "git.changeRemovedSign.text": "≃",

  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.file.hiddenRules": {
      "extensions": ["o", "a", "obj", "pyc"],
      "filenames": ["node_modules", ".git", "__pycache__"],
      "patternMatches": ["^\\."]
  },
  // "explorer.openAction.changeDirectory": false,

  "tsserver.npm": "~/n/bin/npm",
  "tsserver.trace.server": "verbose",
  "tsserver.log": "verbose",

  "prettier.trailingComma": "es5",
  "prettier.semi": false,

  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "python.linting.pylamaEnabled": true,
  "python.linting.pylamaArgs": ["-i", "E501"],

  "snippets.userSnippetsDirectory": "~/dotfiles/nvim/snippets",
  "snippets.ultisnips.pythonPrompt": false,
}
